# Copy to runpod.env and fill in
RUNPOD_API_KEY=
# Preset: vllm-openai or open-webui
PRESET=vllm-openai
# vLLM params
VLLM_MODEL=mistralai/Mistral-7B-Instruct-v0.3
VLLM_MAX_CONTEXT=32768
HUGGING_FACE_HUB_TOKEN=
# Pod settings
POD_NAME=legal-vllm
GPU_QUERY=H100
GPU_COUNT=1
CLOUD_TYPE=SECURE
INTERRUPTIBLE=false
VOLUME_GB=100
CONTAINER_DISK_GB=50
# Cuda versions (optional, comma-separated, e.g., 12.0,12.1)
CUDA_VERSIONS=
